<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="edu.uts.uniapp.view.controller.ExceptionController"
      spacing="10" style="-fx-padding:20;">
    <Label fx:id="titleLabel" style="-fx-font-size:16;-fx-font-weight:bold;"/>
    <Label fx:id="messageLabel" wrapText="true"/>
    <Button text="OK" onAction="#onOk" alignment="CENTER_RIGHT"/>
</VBox>
